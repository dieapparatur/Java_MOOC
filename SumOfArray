
public class SumOfArray {
    public static void main(String[] args) {

        int[] number = {5, 1, 3, 4, 2};

        System.out.println(sumOfNumbersInArray(number));

    }

    public static int sumOfNumbersInArray(int[] array) {
        int sum = 0;
        for (int additions: array) {
            sum += additions;
        }

        return sum;
    }
}
